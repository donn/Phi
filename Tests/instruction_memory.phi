module instruction_memory(
  pc: Input[31..0],
  instruction: Output[31..0]
) {

  //15 rows of 32 bits
  Register[31..0] inst_mem [15];
  
  for i in 0..14 {
  	inst_mem[i] = $interpretFromFile("instructions.txt", i);
  } 

  Wire[4..0] read_address = pc[5..1]; 
  instruction = inst_mem[read_address];

}
