module dFlipFlop(
    a: Input[31..0],
    b: Output[31..0],
    clock: Input,
    reset: Input
) {
    Wire[31..0] v = x;
    Register[31..0] store;

    @ posedge clock | negedge reset {
        if reset {
            store.next = 32b0;
        } else {
            store.next = a;
        }
    }

    b = store;
}