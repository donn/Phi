module mips_32_bit(
    clk: Input,
    rst: Input
){

Wire[5..0] opcode;
Wire RegDst, RegWrite;
Wire ALUSrc;
Wire[1..0] ALUOp;
Wire MemRead, MemWrite, MemtoReg;
Wire Branch, BranchEqual, BranchNotEqual;
Wire Jump;

//processor_control --> input: opcode
processor_control myprocessorcontrol(
    opcode: opcode, 
    RegDst: RegDst, 
    ALUSrc: ALUSrc,
    MemtoReg: MemtoReg, 
    RegWrite: RegWrite,
    MemRead: MemRead, 
    MemWrite: MemWrite,
    Branch: Branch, 
    BranchEqual: BranchEqual, 
    BranchNotEqual: BranchNotEqual,
    ALUOp: ALUOp, 
    Jump: Jump);

//datapath --> output: opcode
datapath mydatapath(
    clk: clk, 
    rst: rst, 
    RegDst: RegDst, 
    ALUSrc: ALUSrc,
    MemtoReg: MemtoReg, 
    RegWrite: RegWrite,
    MemRead: MemRead, 
    MemWrite: MemWrite,
    Branch: Branch, 
    BranchEqual: BranchEqual, 
    BranchNotEqual: BranchNotEqual,
    ALUOp: ALUOp, 
    Jump: Jump, 
    opcode: opcode);


}